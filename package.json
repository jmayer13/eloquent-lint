{
  "name": "eloquestlint",
  "version": "0.0.1",
  "license": "MIT",
  "description": "An extremely simple, pluggable lint",
  "main": "./build/eloquent.js",
  "scripts": {
    "start": "babel-node index.js --presets es2015,stage-3 --source-maps --plugins transform-runtime",
    "run-example": "babel-node example/example.js --presets es2015,stage-0 --source-maps --require babel-polyfill",
    "test": "ava test",
    "build": "babel --presets es2015,stage-0 --plugins transform-runtime ./lib/index.js -o  build/eloquent.js",
    "lint": "eslint lib test example"
  },
  "dependencies": {
    "absolute": "0.0.1",
    "babel-cli": "^6.16.0",
    "babel-runtime": "^6.20.0",
    "clone": "^2.0.0",
    "co-fs-extra": "^1.2.1",
    "is": "^3.1.0",
    "is-utf8": "~0.2.0",
    "lodash": "^4.16.4",
    "recursive-readdir": "^2.0.0",
    "thunkify": "^2.1.2",
    "unyield": "0.0.1",
    "ware": "^1.2.0"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel": "^6.5.2",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-3": "^6.17.0",
    "eslint": "^3.11.1",
    "eslint-config-airbnb-base": "^10.0.0",
    "eslint-plugin-import": "^2.2.0",
    "sinon": "^1.17.6"
  },
  "ava": {
    "files": [
      "test"
    ],
    "source": [
      "**/*.{js,jsx}",
      "!dist/**/*"
    ],
    "concurrency": 5,
    "failFast": true,
    "tap": true,
    "powerAssert": false,
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "babel": "inherit"
  }
}
