{
  "name": "eloquestlint",
  "version": "0.0.1",
  "license": "MIT",
  "description": "An extremely simple, pluggable lint",
  "scripts": {
    "start": "babel-node index.js --presets es2015,stage-0 --source-maps --require babel-polyfill",
    "run-example": "babel-node example/example.js --presets es2015,stage-0 --source-maps --require babel-polyfill",
    "test": "mocha --compilers js:babel-core/register test --require babel-polyfill",
    "lint": "eslint lib test example"
  },
  "dependencies": {
    "absolute": "0.0.1",
    "babel-cli": "^6.16.0",
    "clone": "^2.0.0",
    "co-fs-extra": "^1.2.1",
    "is": "^3.1.0",
    "is-utf8": "~0.2.0",
    "lodash": "^4.16.4",
    "recursive-readdir": "^2.0.0",
    "rimraf": "^2.2.8",
    "thunkify": "^2.1.2",
    "unyield": "0.0.1",
    "ware": "^1.2.0"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "co-mocha": "^1.0.1",
    "eslint": "^3.8.1",
    "eslint-config-airbnb-base": "^9.0.0",
    "eslint-plugin-import": "^2.0.1",
    "mocha": "^3.1.2"
